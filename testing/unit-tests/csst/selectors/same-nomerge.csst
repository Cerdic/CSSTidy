--TEST--
Don't merge multiples occurences of same seletor
--SETTINGS--
merge_selectors = 0
--CSS--
h1 {display: inherit;}
h1,h2 {font-size:1em;}
h1 {font-size:2em;}
--EXPECT--
array (
  'h1' => 
  array (
    'display' => 'inherit',
  ),
  'h1,h2' => 
  array (
    'font-size' => '1em',
  ),
  'h1 ' => 
  array (
    'font-size' => '2em',
  ),
)
