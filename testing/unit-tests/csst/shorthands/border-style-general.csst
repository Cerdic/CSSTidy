--TEST--
Test general border-style optimization
--CSS--
a {
    border-top-style: none;
    border-right-style: solid;
    border-bottom-style: dotted;
    border-left-style: dashed;
}
b {
    border-style: none solid dotted dashed;
}
--EXPECT--
array (
  'a' =>
  array (
    'border-style' => 'none solid dotted dashed',
  ),
  'b' =>
  array (
    'border-style' => 'none solid dotted dashed',
  ),
)
--SETTINGS--
optimise_shorthands=1